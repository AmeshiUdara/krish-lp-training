package queryBuilder;

public class QueryMain{
	
	public static void main(String[] args){
		
		Query q1 = new QueryBuilder().select("*").from("employee").build();
		System.out.println(q1);
		
		Query q2 = new QueryBuilder().select("id, name").from("students").where("id = 'SID234'").build();
		System.out.println(q2);
		
		Query q3 = new QueryBuilder().select("id").where("id = 'SID234'").build();
		System.out.println(q3);
	
	}
	
}