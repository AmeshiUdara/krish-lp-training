package school;

public class Principal extends Handler{
	@Override
	public void handleStudent(Student student){
		
		if(student.getCategories() == Categories.HIGH){
			System.out.println("Diagnose the Patient " + student.getName() + " by the Senior Doctor \n");
		}else{
			System.out.println("Passing the Patient " + student.getName() + " to Higher Authority from Senior Doctor");
			this.successor.handlePatient(student);
		}
		
	}
}
	
	
	
