package drinksFactory;

public class Main {
	public static void main(String[] args) {

		Drinks Nescafe = DrinksFactory.getDrinks(DrinksType.NESCAFE);
		Nescafe.operateMachine("Nescafe");

		Drinks NesTea = DrinksFactory.getDrinks(DrinksType.NESTEA);
		NesTea.operateMachine("NesTea");

		Drinks CocaCola = DrinksFactory.getDrinks(DrinksType.COCACOLA);
		CocaCola.operateMachine("CocaCola");

		Drinks Pepsi = DrinksFactory.getDrinks(DrinksType.PEPSI);
		Pepsi.operateMachine("Pepsi");

	}

}
