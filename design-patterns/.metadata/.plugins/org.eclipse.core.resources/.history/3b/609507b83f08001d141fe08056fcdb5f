package nescafe;

import nescafe.Nescafe;

public class Main {
	public static void main(String[] args){
		
		ManageNescafe singletonManageNescafe = ManageNescafe.getManageNescafe();	
		
		Nescafe singletonNescafe = singletonManageNescafe.getNescafe();
		
		singletonNescafe.printPressTheButton("Good Morning!");
		
		singletonNescafe.displayAvailableList("Tea and Nescafe");
			
	}

}
